    android:layout_height="match_parent"
    tools:context="com.culqi.checkout.CheckoutActivity">

    <WebView
        android:background="#FFFFFF"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/webView"
